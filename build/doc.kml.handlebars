<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
	{{#each .}}
	<Document>
		<name>{{name}}, auto-generated by kml-maker</name>
		<Folder>
			<name>Generated by kml-maker https://github.com/smeinel/kml-maker</name>
			<ScreenOverlay>
				<name>Download at: https://github.com/smeinel/kml-maker</name>
				<visibility>1</visibility>
			</ScreenOverlay>
		</Folder>
		{{#each folders}}
		<Folder>
			<name>{{name}}</name>
			{{#each placemarks}}
			<Placemark>
				<name>{{name}}</name>
				<description>{{{description}}}</description>
				<Style>
					<LineStyle>
						<color>ff0000ff</color>
						<colorMode>normal</colorMode>
						<width>2.25</width>
					</LineStyle>
					<PolyStyle>
						<color>00000000</color>
						<colorMode>normal</colorMode>
						<fill>1</fill>
						<outline>1</outline>
					</PolyStyle>
				</Style>
				<Polygon>
					<outerBoundaryIs>
						<LinearRing>
							<coordinates>{{west}},{{north}} {{east}},{{north}} {{east}},{{south}} {{west}},{{south}} {{west}},{{north}}</coordinates>
						</LinearRing>
					</outerBoundaryIs>
				</Polygon>
			</Placemark>
				{{#each processed_file_data.tile_data}}
				<GroundOverlay>
					<name>Map tile{{@index}}</name>
					<description>Map tile{{@index}}</description>
					<Icon>
						<href>files/{{img}}</href>
						<viewBoundScale>0.75</viewBoundScale>
					</Icon>
					<LatLonBox>
						<north>{{north}}</north>
						<south>{{south}}</south>
						<east>{{east}}</east>
						<west>{{west}}</west>
					</LatLonBox>
				</GroundOverlay>
				{{/each}}
			{{/each}}
		</Folder>
		{{/each}}
	</Document>
	{{/each}}
</kml>